

-- 회원 시퀀스
CREATE SEQUENCE member_seq
    INCREMENT BY 1
    START WITH 1
    NOMAXVALUE
    NOCYCLE
    NOCACHE;

-- 권한 시퀀스
CREATE SEQUENCE memberAuthority_seq
    INCREMENT BY 1
    START WITH 1
    NOMAXVALUE
    NOCYCLE
    NOCACHE;
    
